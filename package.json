{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev-ssl": "concurrently --kill-others -n \"next,cors,ssl-p,cors-p\" \"next dev\"  \"npm:cors-forwarder\" \"npm:ssl-proxy\" \"npm:cors-proxy\" ",
    "ssl-proxy": "local-ssl-proxy --key ./keys/localhost-key.pem --cert ./keys/localhost.pem --source 3001 --target 3000",
    "cors-proxy": "local-ssl-proxy --key ./keys/localhost-key.pem --cert ./keys/localhost.pem --source 3003 --target 3002",
    "cors-forwarder": "npx lcp --proxyUrl https://waifus-api.nemusona.com/ --port 3002 --proxyPartial /",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "setup-ssl": "mkcert -install && mkdir keys && mkcert -cert-file ./keys/localhost.pem -key-file ./keys/localhost-key.pem localhost"
  },
  "dependencies": {
    "@types/node": "20.3.0",
    "@types/react": "18.2.11",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "d3": "^7.8.5",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.5",
    "next": "^13.4.6",
    "png-chunk-text": "^1.0.0",
    "png-chunks-encode": "^1.0.0",
    "png-chunks-extract": "^1.0.0",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.44.3",
    "react-hot-toast": "^2.4.1",
    "react-query": "^3.39.3",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "local-cors-proxy": "^1.1.0",
    "local-ssl-proxy": "^2.0.5"
  }
}
